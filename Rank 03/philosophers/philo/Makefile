	for c in "üëª" " " "D" "i" "e" "g" "o" "'" "s" " " "P" "h" "i" "l" "o" "s" "o" "p" "h" "e" "r" "s" " " "s" "u" "c" "c" "e" "s" "s" "f" "u" "l" "l" "y" " " "c" "o" "m" "p" "i" "l" "e" "d" " " "!" " " "üëª"; do \
