NAME = philo

SRC_DIR				= srcs/
OBJ_DIR				= obj/

CC					= cc
CFLAGS				= -Wall -Werror -Wextra 
# -g -pthread -fsanitize=thread
RM					= rm -f

SRCS = $(SRC_DIR)cleanup_utils.c $(SRC_DIR)config_parser.c \
		$(SRC_DIR)dining_setup.c $(SRC_DIR)main.c \
		$(SRC_DIR)philosopher_life.c $(SRC_DIR)simulation_monitor.c \
		$(SRC_DIR)philosopher_life2.c 
				
OBJ 				= $(patsubst $(SRC_DIR)%.c,$(OBJ_DIR)%.o,$(SRCS))

start:
					@make all --no-print-directory

all: 				logo $(NAME)

