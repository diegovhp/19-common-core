	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚£∑‚†ô‚£¶‚¢ª‚£Ω‚£ø‚¢ª‚£Ω‚£ø‚£ø‚°ü‚£ø‚£Ø‚£ø‚£ø‚£æ‚£ø‚£ø‚¢ª‚£Ω‚£ø‚£ø‚£∑‚£ø‚£Ø‚£ø‚£ø‚£Ω‚£ø‚£ø‚¢ª‚£Ω‚£ø‚£Ω‚£æ‚£ø‚£ø‚†ã‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†∏‚£∑‚£ä‚†∂‚£≠‚¢ª‚£ø‚£ø‚£Ø‚£∑‚£ø‚£ø‚£ø‚°ø‚£ø‚£ø‚¢ø‚£ª‚£ø‚£ø‚£ø‚£ø‚£ø‚£ª‚£ø‚¢ø‚°Ω‚¢è‚°≥‚¢¨‚¢£‚£ì‚¢Æ‚°ª‚¢ø‚£ø‚†è‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†ô‚¢ø‚£æ‚£º‚£ø‚£≥‚£ø‚£ø‚£Ø‚°∑‚£ø‚°∑‚£ø‚£ø‚¢ø‚£ø‚£ø‚£ø‚¢ø‚£ø‚£ª‚£ø‚£ø‚¢Ø‚°õ‚£ú‚¢£‚°ù‚£é‚¢≥‚¢¨‚†≥‚£ù‚¢Ø‚°ø‚°Ü‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†à‚¢ª‚£∑‚£ø‚£∑‚£ø‚£ø‚†õ‚¢∑‚£ª‚°Ω‚£Ø‚¢ø‚£ø‚°æ‚£ø‚°ø‚£ü‚£ø‚°ø‚£û‚£Ø‚¢≥‚£å‚¢≥‚°ú‚°¨‚¢ì‚¢Æ‚£π‚°û‚°ø‚£ú‚°∑‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†ô‚†ü‚¢ø‚°è‚†ô‚†Ä‚†Ä‚†à‚†ô‚†ø‚¢ø‚£æ‚£ü‚£ø‚£Ω‚£ø‚£ª‚£Ω‚°ª‚£ú‚¢Ø‚£≤‚¢Ø‚°õ‚£¥‚¢ª‚£ã‚†∑‚£©‚¢ì‚°é‚°ó‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†ª‚£ø‚£ø‚¢ø‚£ø‚†è‚†â‚†ô‚¢Ø‚¢é‚°≥‚¢é‚£µ‚£è‚£∂‚£Ø‚¢í‚†•‚£ã‚£º‚†É‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†à‚†ô‚†ø‚†è‚†Ä‚†Ä‚†Ä‚†à‚¢ß‚°ù‚£æ‚£ø‚£ø‚£ø‚£ø‚£æ‚£ø‚°º‚†É‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†à‚¢ø‚£ø‚£ø‚£≥‚°ø‚£û‚°ø‚†ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	echo "$(RHYTHM)‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†ô‚†õ‚†õ‚†õ‚†â‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä$(RESET)"; \
	} | while IFS= read -r line; do \
