		printf "$$c"; \
		sleep 0.03; \
	done; \
	printf "\n\n"; \
	{ \
	echo "\033[38;2;162;156;187m		     :::::::::    :::    :::    :::    :::       :::::::::    ::::::::    :::::::::     :::::::::    :::    :::    :::::::::    :::::::::       ::::::::"; \
