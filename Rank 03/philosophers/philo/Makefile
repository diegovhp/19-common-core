$(NAME): 			$(OBJ)
					@$(CC) $(CFLAGS) $(OBJ) -o $(NAME)
					@$(MAKE) progress_bar --no-print-directory
					@$(MAKE) gengar	--no-print-directory
					
$(OBJ_DIR)%.o:		$(SRC_DIR)%.c
					@mkdir -p $(@D)
					@$(CC) $(CFLAGS) -c $< -o $@

clean:
	@tput civis; \
	printf "\033[38;2;241;178;172m"; \
	for c in "üßπ" " " "C" "l" "e" "a" "n" "i" "n" "g" " " "üßπ"; do \
		printf "$$c"; \
		sleep 0.03; \
	done; \
	\
	printf "\n\n\033[38;2;241;178;172m‚ï≠‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚ïÆ\n"; \
	printf " \033[38;2;241;178;172m‚îÇ\033[38;2;125;120;163m"; \
	i=0; \
	while [ $$i -le 40 ]; do \
		sleep 0.01; \
		printf "‚ñà"; \
		i=$$((i + 1)); \
	done; \
	printf "\033[38;2;241;178;172m‚îÇ\n"; \
	printf "‚ï∞‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚ïØ\033[0m\n"; \
	tput cnorm
	@$(RM) -r $(OBJ_DIR)

fclean:
	@tput civis; \
	printf "\033[38;2;241;178;172m"; \
	for c in "üßπ" " " "F" "u" "l" "l" "y" " " "c" "l" "e" "a" "n" "i" "n" "g" " " "üßπ"; do \
		printf "$$c"; \
		sleep 0.03; \
	done; \
	\
	printf "\n\n\033[38;2;241;178;172m‚ï≠‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚ïÆ\n"; \
	printf " \033[38;2;241;178;172m‚îÇ\033[38;2;125;120;163m"; \
	i=0; \
	while [ $$i -le 40 ]; do \
		sleep 0.01; \
		printf "‚ñà"; \
		i=$$((i + 1)); \
	done; \
	printf "\033[38;2;241;178;172m‚îÇ\n"; \
	printf "‚ï∞‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚ïØ\033[0m\n"; \
	printf "\n\n\033[38;2;241;178;172m"; \
	for c in "‚úÖ" " " "D" "o" "n" "e" " " "‚úÖ"; do \
		printf "$$c"; \
		sleep 0.03; \
	done; \
	printf "\n"; \
	tput cnorm
	@$(RM) -r $(OBJ_DIR)
	@$(RM) $(NAME)

